{
  "name": "thread-sleep-compat",
  "version": "0.2.25",
  "description": "thread-sleep the runs on multiple versions of node",
  "keywords": [
    "thread-sleep",
    "thread",
    "sleep"
  ],
  "homepage": "https://github.com/kmalakoff/thread-sleep-compat",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/thread-sleep-compat.git"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.cts",
  "files": [
    "dist",
    "assets",
    "out"
  ],
  "scripts": {
    "build": "tsds build",
    "build-binaries": "node scripts/build-binaries.mjs",
    "format": "biome check --write --unsafe src/ test/ scripts/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "nan": "^2.16.0",
    "node-abi": "2.30.1",
    "semver": "^5.7.1",
    "thread-sleep": "^2.2.0"
  },
  "devDependencies": {
    "@types/mocha": "*",
    "@types/node": "*",
    "cmake-js": "*",
    "cross-spawn-cb": "*",
    "fast-extract": "*",
    "lodash.find": "*",
    "node-filename-to-dist-paths": "*",
    "node-semvers": "*",
    "prebuild": "*",
    "queue-cb": "*"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts",
    "targets": [
      "cjs"
    ]
  }
}
